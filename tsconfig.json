{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "jsx": "react",
      "declaration": true,
      "declarationMap": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "moduleResolution": "Node",
      "outDir": "dist",
      "rootDir": "src"
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  